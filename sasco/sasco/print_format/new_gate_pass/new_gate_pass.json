 "html": "{# ---------------------------------------------------------\n   Gate Pass \u2013 Standard Print Format (Letterhead Supported)\n   --------------------------------------------------------- #}\n\n{{ letter_head }}\n\n<h3 style=\"text-align: center; margin-top: 10px;\">\n    Gate Pass\n</h3>\n\n<hr>\n\n<table class=\"table table-condensed\" style=\"margin-bottom: 10px;\">\n    <tr>\n        <td>\n            <small class=\"text-muted\">Gate Pass No</small><br>\n            <strong>{{ doc.name }}</strong>\n        </td>\n        <td>\n            <small class=\"text-muted\">Date</small><br>\n            <strong>{{ doc.gate_out_time }}</strong>\n        </td>\n    </tr>\n    <tr>\n        <td>\n            <small class=\"text-muted\">Customer</small><br>\n            <strong>{{ doc.delivery_details[0].customer or \"-\" }}</strong>\n        </td>\n        <td>\n            <small class=\"text-muted\">Vehicle No</small><br>\n            <strong>{{ doc.shipment_vehicle or \"-\" }}</strong>\n        </td>\n    </tr>\n    <tr>\n        <td>\n            <small class=\"text-muted\">Driver Name</small><br>\n            <strong>{{ doc.driver or \"-\" }}</strong>\n        </td>\n        <td>\n            <small class=\"text-muted\">Status</small><br>\n            <strong>{{ doc.docstatus == 1 and \"Submitted\" or \"Draft\" }}</strong>\n        </td>        \n    </tr>\n</table>\n\n<!-- DELIVERY NOTES -->\n<h5 style=\"margin-top: 15px;\">Delivery Notes</h5>\n\n<table class=\"table table-bordered table-condensed\">\n    <thead>\n        <tr>\n            <th style=\"width: 8%;\">#</th>\n            <th>Delivery Note</th>\n            <th>Customer</th>\n        </tr>\n    </thead>\n    <tbody>\n        {% for row in doc.delivery_details %}\n        <tr>\n            <td>{{ loop.index }}</td>\n            <td>{{ row.delivery_note }}</td>\n            <td>{{ row.customer }}</td>\n        </tr>\n        {% endfor %}\n    </tbody>\n</table>\n\n<!-- ITEMS -->\n<h5 style=\"margin-top: 15px;\">Items</h5>\n\n{% set ns = namespace(total_qty=0) %}\n\n<table class=\"table table-bordered table-condensed\">\n    <thead>\n        <tr>\n            <th style=\"width: 5%;\">#</th>\n            <th style=\"width: 18%;\">Item Code</th>\n            <th>Item Name</th>\n            <th style=\"width: 8%;\" class=\"text-right\">Qty</th>\n            <th style=\"width: 8%;\">UOM</th>\n            <th style=\"width: 15%;\">Warehouse</th>\n        </tr>\n    </thead>\n    <tbody>\n        {% for item in doc.detail_items %}\n        {% set ns.total_qty = ns.total_qty + (item.qty or 0) %}\n        <tr>\n            <td>{{ loop.index }}</td>\n            <td>{{ item.item_code }}</td>\n            <td>{{ item.item_name or \"\" }}</td>\n            <td class=\"text-right\">{{ \"%.2f\"|format(item.qty or 0) }}</td>\n            <td>{{ item.uom }}</td>\n            <td>{{ item.warehouse }}</td>\n            \n        </tr>\n        {% endfor %}\n    </tbody>\n</table>\n\n\n<table class=\"table table-condensed\" style=\"width: 40%; float: right; margin-top: 5px;\">\n    <tr>\n        <th>Total Quantity</th>\n        <th class=\"text-right\">{{ \"%.2f\"|format(ns.total_qty) }}</th>\n    </tr>\n</table>\n\n\n<div style=\"clear: both;\"></div>\n\n<!-- SIGNATURES -->\n<table class=\"table table-condensed\" style=\"margin-top: 45px;\">\n    <tr>\n        <td style=\"text-align: center;\">\n            ___________________________<br>\n            Prepared By\n        </td>\n        <td style=\"text-align: center;\">\n            ___________________________<br>\n            Security\n        </td>\n        <td style=\"text-align: center;\">\n            ___________________________<br>\n            Driver / Receiver\n        </td>\n    </tr>\n</table>\n\n\n<p class=\"text-center text-muted\" style=\"font-size: 9px;\">\n    System Generated Gate Pass\n</p>\n\n\n",
